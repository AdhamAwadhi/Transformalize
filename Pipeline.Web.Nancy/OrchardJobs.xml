<add name="Service">

  <response>
    <add request="Configuration" status="200" message="Ok" time="1" />
  </response>

  <connections>
    <add name="input" provider="sqlserver" server="localhost" database="Orchard192" />
  </connections>

  <entities>
    <add name="Pipe" alias="Jobs">
      <fields>
        <add label="Id" name="id" type="int" primary-key="true" />
        <add label="Title" name="title" length="128" />
        <add label="Job" input="false" name="job" length="128" t="copy(id).format(/api/job/{0})" />
        <add label="Meta" input="false" name="meta" length="128" t="copy(id).format(/api/meta/{0})" />
        <add label="Page" input="false" name="page" length="128" t="copy(id).format(/api/page/{0}/1)" />
        <add label="Edit" input="false" name="edit" length="128" t="copy(id).format(http://localhost/Orchard192/Admin/Contents/Edit/{0})" />
      </fields>
    </add>
  </entities>

</add>
